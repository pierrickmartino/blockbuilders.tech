{
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2021"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
  }
}
